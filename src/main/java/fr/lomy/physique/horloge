package com.mycompany.lomy;

/**
 *
 * @author valentinwendling
 */
public class horloge {
    private Integer temps;

    public horloge(Integer temps) {
        this.temps = temps;
    }

    public Integer getTemps() {
        return temps;
    }
    
    public void incremente(){
        this.temps += 1;
    }
}
